{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": [
            "> 1%",
            "last 2 versions"
          ]
        }
      }
    ],
    "flow",
    "react"
  ],
  "plugins": [
    "transform-runtime",
    "react-hot-loader/babel"
  ],
  "comments": false,
  "minified": true
}